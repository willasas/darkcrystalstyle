{
  "name": "dcjs",
  "version": "1.0.0",
  "description": "a js tools for webdevelopment",
  "keywords": [
    "js",
    "utils",
    "functions",
    "file",
    "browser",
    "cookies"
  ],
  "main": "src/index.js",
  "scripts": {
    "start": "npm-run-all src/index.js",
    "build": "npm-run-bulid src/index.js",
    "test": "node test/test.js | tap-spec"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {},
      "postcss-px-to-viewport": {
        "unitToConvert": "px",
        "viewportWidth": 1920,
        "unitPrecision": 5,
        "propList": [
          "*",
          "!letter-spacing"
        ],
        "viewportUnit": "vw",
        "fontViewportUnit": "vw",
        "selectorBlackList": [
          "/^body$/"
        ],
        "minPixelValue": 1,
        "mediaQuery": false,
        "replace": true,
        "exclude": "node_modules",
        "landscape": false,
        "landscapeUnit": "vw",
        "landscapeWidth": 1080
      }
    }
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "autoprefixer": "^10.4.0",
    "chalk": "^5.0.0",
    "chokidar": "^3.5.2",
    "commander": "^8.3.0",
    "concat": "^1.0.3",
    "download": "^8.0.0",
    "fs-extra": "^10.0.0",
    "glob-parent": "^6.0.2",
    "handlebars": "^4.7.7",
    "inquirer": "^8.2.0",
    "live-server": "^1.2.1",
    "log-symbols": "^5.1.0",
    "npm-run-all": "^4.1.5",
    "ora": "^6.0.1",
    "postcss": "^8.4.1",
    "postcss-cli": "^9.0.2",
    "sass": "^1.26.8",
    "update-notifier": "^5.1.0"
  },
  "dependencies": {
    "d3-color": "^3.0.1",
    "postcss-px-to-viewport": "^1.1.1",
    "reduce-function-call": "^1.0.3",
    "stylelint": "^14.1.0",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.0.0",
    "parcel": "^2.0.0"
  }
}
